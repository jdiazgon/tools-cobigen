<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<templatesConfiguration xmlns="http://capgemini.com/devonfw/cobigen/TemplatesConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1">

  <templates>
    <templateExtension ref="${variables.className}.java" mergeStrategy="javamerge"/>
  </templates>

  <templateScans>
    <templateScan templatePath="templates/java" destinationPath="src/main/java"/>
  </templateScans>

  <increments>
    <increment name="xml_uml_ea" description="XML UML EA">
      <templateRef ref="${variables.className}.java"/>
    </increment>
  </increments>
</templatesConfiguration>
